\frametitle{Tabela hash (endereçamento aberto) - Remoção}
\begin{verbatim}
REMOÇÃO(TABELA, CHAVE) {
    I = 0

    FAÇA
        ENDEREÇO = HASH'( CHAVE, I )

        SE TABELA[ ENDEREÇO ] = CHAVE
            TABELA[ ENDEREÇO ] = REMOVIDO
            return TRUE
        SENÃO
            I = I + 1
    ATÉ QUE TABELA[ENDEREÇO] = VAZIO OU I = M

    return FALSE
}
\end{verbatim}
