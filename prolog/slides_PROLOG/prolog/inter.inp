
main :-    inter( [a,b,c] ,  [a,b,c,d,e] , X1 ), write(X1) , nl,
           inter( [a,e] , [a,a,c,d,d,d,d,d,e,e,e,e,e], X2), write(X2), nl,
           inter( [a,b,c] , [], X3), write(X3 ), nl,
           inter( [a,b,c,d,e] ,  [a,b,c,d,e], X4 ), write(X4), nl,
           inter( [b, e] , [e,a,b, b,b,b,b,b,c,b,b,bb, d,ee,e,e,e,e,e,e], X5), write(X5), nl,
           inter( [a, b] , [c,x,2,3,4] , X6 ),  write(X6) , nl.
