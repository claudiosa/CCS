/******************* AUX PREDICATES **********/
membchk_array(Element, Array) => 
    List = to_list(Array),
    membchk(Element,List).


get_top(Queue) = R =>
    R = last(Queue).

enqueue(V,Queue) = R => 
    R = [V]++Queue.

dequeue(Queue) = R =>
    if (len(Queue) > 0) then
        Front = last(Queue),
        append(New_Queue, [Front], Queue),
        R = (Front, New_Queue)
    else
        printf("\n The queue is empty"),
        %exit()
        R = (9999,[])
    end.


