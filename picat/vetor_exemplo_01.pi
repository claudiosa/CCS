main =>
 L = [(A, I) : A in [a, b], I in 1 .. 2],
 V = {(I, A) : A in [a, b], I in 1 .. 2},
 printf("\nLista: %w  \nVetor: %w\n", L, V),
 imp_vetor(V),
 imp_vetor(L).

imp_vetor (M) => 
Tam = M.length,  %% tamanho de M
  nl,
   foreach(I in 1  .. Tam )
     printf("V_ou_L(%d):%w \t" , I, M[I] )
    end,
  nl.
