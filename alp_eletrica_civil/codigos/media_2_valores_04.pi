%% reading a integer in Picat
main =>
	printf("\n  >> Oi Turma de ALP << \n "),
	printf("\n  Digite dois números:  "),
	%X = read_int(stdin),
	%%% LENDO
	N_real_01 = read_real(),
	N_real_02 = read_real(),
	Media = (N_real_01+N_real_02)/2,
	%% Escrevendo
	printf("\n  A média é: %6.2f ", Media ),
	printf("\n  >> THE END <<\n ").